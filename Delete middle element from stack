#include <bits/stdc++.h>
void solve(stack<int> &inputStack, int n, int count) {
  if (count == size/2) {
  inputStack.pop();
  return;
  }
  int num=inputStack.top();
  inputStack.pop();
  solve(inputStack, n, count++);
  inputStack.push(num);
}
