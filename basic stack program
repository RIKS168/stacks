// stack using stl 
#include <iostream>
#include <stack>

using namespace std;

int main() {
  // Create a stack of integers
  stack<int> s;

  // Push some elements onto the stack
  s.push(1);
  s.push(2);
  s.push(3);

  // Pop an element off the stack
  int top = s.top();
  s.pop();

  // Print the top element of the stack
  cout << "The top element of the stack is: " << top << endl;
  return 0;
}
